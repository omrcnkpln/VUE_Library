{
  "name": "vue3-vitest-testinglibrary",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@testing-library/vue": "^6.6.1",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.27.1",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "happy-dom": "^8.1.4",
    "jsdom": "^20.0.3",
    "msw": "^0.49.2",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vitest": "^0.25.8",
    "vue-tsc": "^1.0.12",
    "whatwg-fetch": "^3.6.2"
  }
}
